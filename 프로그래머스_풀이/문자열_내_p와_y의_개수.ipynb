      "source": [
        "def solution(s):\n",
        "    dic = { 'p' : 0,\n",
        "          'y' : 0}\n",
        "    for i in s.lower():\n",
        "        if i in dic:\n",
        "            dic[i] += 1\n",
        "    if dic['p'] == dic['y']:\n",
        "        return True\n",
        "    else:\n",
        "        return False"
      ]
    }
  ]
