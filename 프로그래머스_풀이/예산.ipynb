      "source": [
        "def solution(d, budget):\n",
        "    answer = len(d)\n",
        "    sum_d = sum(d)\n",
        "    stack = sorted(d)\n",
        "    while sum_d > budget:\n",
        "        sum_d -= stack[-1]\n",
        "        stack.pop()\n",
        "        answer -= 1\n",
        "    return answer"
      ]
    }
  ]
