      "source": [
        "def solution(strings, n):\n",
        "    answer = []\n",
        "    for i in strings:\n",
        "        answer.append(i[n:n+1])\n",
        "    answer = set(answer)\n",
        "    answer = sorted(list(answer))\n",
        "    \n",
        "    stack = [i for k in range(len(answer)) for i in sorted(strings) if i[n] == answer[k]]\n",
        "        \n",
        "    return stack"
      ]
    }
  ]
}