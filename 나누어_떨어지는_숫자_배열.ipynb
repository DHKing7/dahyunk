      "source": [
        "def solution(arr, divisor):\n",
        "    answer = []\n",
        "    if divisor == 1:\n",
        "        return sorted(arr)\n",
        "    for i in arr:\n",
        "        if i % divisor == 0:\n",
        "            answer.append(i)\n",
        "    if len(answer) == 0:\n",
        "        return [-1]\n",
        "    return sorted(answer)"
      ]
    }
  ]
}