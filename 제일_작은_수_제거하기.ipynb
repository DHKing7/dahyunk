        "def solution(arr):\n",
        "    answer = []\n",
        "    if len(arr) == 0 or arr == [10]:\n",
        "        return [-1]\n",
        "    else:\n",
        "        for i in arr:\n",
        "            answer.append(i)\n",
        "        answer.sort()\n",
        "        arr.remove(answer[0])\n",
        "        return arr"
      ]
    }
  ]
