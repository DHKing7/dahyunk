      "source": [
        "def solution(nums):\n",
        "    stack = []\n",
        "    for i in nums:\n",
        "        if len(stack) == 0:\n",
        "            stack.append(i)\n",
        "        else:\n",
        "            if i not in stack:\n",
        "                stack.append(i)\n",
        "    if len(nums)/2 <= len(stack):\n",
        "        return len(nums)/2\n",
        "    else:\n",
        "        return len(stack)"
      ]
    }
  ]
